{
  "name": "job-management-server",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "build": "echo 'No build step needed for JS'",
    "start": "node dist/app.js",
    "dev": "node dist/app.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^16.18.126",
    "nodemon": "^2.0.22",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": "22.x"
  },
  "vercel": {
    "version": 2,
    "builds": [
      {
        "src": "dist/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/app.js"
      }
    ],
    "outputDirectory": "dist",
    "rootDirectory": "server"
  }
}
